<div{{ attributes.addClass('block', 'jumbotron', 'text-xs-center', 'clearfix') }}>
	{{ title_prefix }}
	{% if label %}
	<h2{{ title_attributes.addClass('block-title') }}>{{ label }}</h2>
	{% endif %}
	{{ title_suffix }}

	{% block content %}
	<div class="container">
	{{ content }}
	</div>
	{% endblock %}
</div>